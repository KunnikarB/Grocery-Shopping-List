{
  "name": "shopping-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "http://{github-kunnikarb}.github.io/{Grocery-Shopping-List}",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
